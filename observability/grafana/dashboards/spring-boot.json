{
  "__inputs": [],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" }
  ],
  "annotations": { "list": [] },
  "description": "Stock Brokerage – Spring Boot JVM, HTTP and database metrics via Micrometer + Prometheus",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "HTTP Traffic",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Total HTTP requests per second (all endpoints)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "id": 1,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{application=\"stock-brokerage\"}[$__rate_interval])) by (uri, method)",
          "legendFormat": "{{method}} {{uri}}",
          "refId": "A"
        }
      ],
      "title": "HTTP Request Rate",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "HTTP 4xx and 5xx error rates",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" },
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "4xx" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },
      "id": 2,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{application=\"stock-brokerage\", status=~\"4..\"}[$__rate_interval]))",
          "legendFormat": "4xx",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{application=\"stock-brokerage\", status=~\"5..\"}[$__rate_interval]))",
          "legendFormat": "5xx",
          "refId": "B"
        }
      ],
      "title": "HTTP Error Rate",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "HTTP request latency percentiles (p50, p95, p99)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 5 },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
      "id": 3,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_server_requests_seconds_bucket{application=\"stock-brokerage\"}[$__rate_interval])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{application=\"stock-brokerage\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_server_requests_seconds_bucket{application=\"stock-brokerage\"}[$__rate_interval])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "title": "HTTP Latency (P50 / P95 / P99)",
      "type": "timeseries"
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 101,
      "title": "JVM",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "JVM heap memory used vs. committed vs. max",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "bytes"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "max" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "dark-red" } },
              { "id": "custom.lineStyle", "value": { "dash": [10, 10], "fill": "dash" } }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 10 },
      "id": 4,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(jvm_memory_used_bytes{application=\"stock-brokerage\", area=\"heap\"})",
          "legendFormat": "used",
          "refId": "A"
        },
        {
          "expr": "sum(jvm_memory_committed_bytes{application=\"stock-brokerage\", area=\"heap\"})",
          "legendFormat": "committed",
          "refId": "B"
        },
        {
          "expr": "sum(jvm_memory_max_bytes{application=\"stock-brokerage\", area=\"heap\"})",
          "legendFormat": "max",
          "refId": "C"
        }
      ],
      "title": "JVM Heap Memory",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "JVM non-heap (metaspace, code cache, etc.)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "bytes"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 10 },
      "id": 5,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(jvm_memory_used_bytes{application=\"stock-brokerage\", area=\"nonheap\"}) by (id)",
          "legendFormat": "{{id}}",
          "refId": "A"
        }
      ],
      "title": "JVM Non-Heap Memory",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "GC pause durations per GC action",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 10 },
      "id": 6,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_sum{application=\"stock-brokerage\"}[$__rate_interval])",
          "legendFormat": "{{action}} ({{cause}})",
          "refId": "A"
        }
      ],
      "title": "GC Pause Time",
      "type": "timeseries"
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 102,
      "title": "System & Database",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "JVM process CPU usage (0–1)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "blue" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 19 },
      "id": 7,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } },
      "targets": [
        {
          "expr": "process_cpu_usage{application=\"stock-brokerage\"}",
          "legendFormat": "JVM process CPU",
          "refId": "A"
        },
        {
          "expr": "system_cpu_usage{application=\"stock-brokerage\"}",
          "legendFormat": "System CPU",
          "refId": "B"
        }
      ],
      "title": "CPU Usage",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "HikariCP database connection pool – active, idle, pending",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 19 },
      "id": 8,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "hikaricp_connections_active{application=\"stock-brokerage\"}",
          "legendFormat": "active",
          "refId": "A"
        },
        {
          "expr": "hikaricp_connections_idle{application=\"stock-brokerage\"}",
          "legendFormat": "idle",
          "refId": "B"
        },
        {
          "expr": "hikaricp_connections_pending{application=\"stock-brokerage\"}",
          "legendFormat": "pending",
          "refId": "C"
        },
        {
          "expr": "hikaricp_connections{application=\"stock-brokerage\"}",
          "legendFormat": "total",
          "refId": "D"
        }
      ],
      "title": "HikariCP Connection Pool",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "SLF4J/Logback log events by level – useful for spotting ERROR spikes",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "cps"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "error" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }]
          },
          {
            "matcher": { "id": "byName", "options": "warn" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 19 },
      "id": 9,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "rate(logback_events_total{application=\"stock-brokerage\"}[$__rate_interval])",
          "legendFormat": "{{level}}",
          "refId": "A"
        }
      ],
      "title": "Log Events by Level",
      "type": "timeseries"
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 103,
      "title": "Threads & Uptime",
      "type": "row"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "JVM thread states",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 28 },
      "id": 10,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "jvm_threads_states_threads{application=\"stock-brokerage\"}",
          "legendFormat": "{{state}}",
          "refId": "A"
        }
      ],
      "title": "JVM Threads",
      "type": "timeseries"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Application uptime",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "green" },
          "unit": "s",
          "mappings": []
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 28 },
      "id": 11,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "auto", "textMode": "value_and_name", "colorMode": "background" },
      "targets": [
        {
          "expr": "process_uptime_seconds{application=\"stock-brokerage\"}",
          "legendFormat": "Uptime",
          "refId": "A"
        }
      ],
      "title": "Process Uptime",
      "type": "stat"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Total HTTP requests served since startup",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "blue" },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 28 },
      "id": 12,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "auto", "textMode": "value_and_name", "colorMode": "background" },
      "targets": [
        {
          "expr": "sum(http_server_requests_seconds_count{application=\"stock-brokerage\"})",
          "legendFormat": "Total Requests",
          "refId": "A"
        }
      ],
      "title": "Total HTTP Requests",
      "type": "stat"
    },

    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "JVM start time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "purple" },
          "unit": "dateTimeAsIso"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 28 },
      "id": 13,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "auto", "textMode": "value_and_name", "colorMode": "background" },
      "targets": [
        {
          "expr": "process_start_time_seconds{application=\"stock-brokerage\"} * 1000",
          "legendFormat": "Start time",
          "refId": "A"
        }
      ],
      "title": "Process Start Time",
      "type": "stat"
    }

  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["spring-boot", "jvm", "micrometer"],
  "templating": {
    "list": [
      {
        "current": {},
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "definition": "label_values(jvm_memory_used_bytes{application=\"stock-brokerage\"}, instance)",
        "hide": 0,
        "includeAll": true,
        "label": "Instance",
        "multi":  true,
        "name":   "instance",
        "options": [],
        "query": {
          "query": "label_values(jvm_memory_used_bytes{application=\"stock-brokerage\"}, instance)",
          "refId": "StandardVariableQuery"
        },
        "refresh": 2,
        "regex":   "",
        "sort":    1,
        "type":    "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Stock Brokerage – Spring Boot",
  "uid": "stock-brokerage-sb",
  "version": 1
}
